{
  "name": "mon-pix",
  "version": "2.0.0",
  "description": "Plateforme d'évaluation et de certification des compétences numériques à l'usage de tous les citoyens francophones.",
  "license": "AGPL-3.0",
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "author": "Team Pix",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "ci:test": "COVERAGE=true ember test --reporter dot",
    "clean": "rm -rf tmp dist node_modules",
    "coverage": "npm run coverage:check && npm run coverage:convert && npm run coverage:rename",
    "coverage:check": "COVERAGE=true ember test --reporter dot",
    "coverage:convert": "node ../scripts/lcov-path-converter $PWD",
    "coverage:rename": "mv coverage/lcov.info ../coverage/mon-pix_lcov.info",
    "deploy:integration": "./scripts/deploy.sh integration",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "dev": "ember server --proxy",
    "lint": "eslint app mirage tests",
    "lint:fix": "npm run lint --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "scalingo-background-job": "echo 'nothing to do'",
    "scalingo-postbuild": "ember build --environment $BUILD_ENVIRONMENT",
    "scalingo-post-ra-creation": "echo 'nothing to do'",
    "scalingo-start": "/app/scripts/start_scalingo_nginx.sh",
    "start": "ember server",
    "test": "ember test --reporter dot",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "devDependencies": {
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^0.6.3",
    "bootstrap": "^3.3.7",
    "broccoli-asset-rev": "^2.7.0",
    "chai-dom": "^1.8.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.2",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.1.4",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "^6.17.0",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-circleci": "^0.1.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^2.2.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^2.0.4",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-mirage": "^0.4.9",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sass": "^7.2.0",
    "ember-cli-sentry": "^3.0.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-showdown": "^4.4.4",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-click-outside": "^0.1.11",
    "ember-collapsible-panel": "^3.1.1",
    "ember-component-focus": "^0.2.3",
    "ember-data": "^3.4.0",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-group-by": "^0.0.5",
    "ember-keyboard": "^3.0.2",
    "ember-load-initializers": "^1.1.0",
    "ember-lodash": "^4.18.0",
    "ember-math-helpers": "^2.6.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-metrics": "^0.12.1",
    "ember-modal-dialog": "^2.4.1",
    "ember-moment": "^7.7.0",
    "ember-resolver": "^4.5.6",
    "ember-responsive": "^2.0.5",
    "ember-routable-modal": "^0.6.0",
    "ember-route-action-helper": "^2.0.6",
    "ember-side-menu": "^0.0.15",
    "ember-simple-auth": "^1.7.0",
    "ember-sinon": "^1.0.1",
    "ember-source": "^3.4.0",
    "ember-tabbable": "^0.1.3",
    "ember-truth-helpers": "^2.1.0",
    "ember-xregexp": "^1.0.2",
    "eslint": "^4.19.1",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-mocha": "^5.2.0",
    "js-yaml": "^3.12.0",
    "loader.js": "^4.7.0",
    "showdown": "^1.8.6"
  },
  "private": false
}
