    "ember-route-action-helper": "^2.0.7",
    "ember-simple-auth": "^1.8.2",
    "ember-sinon": "^1.0.1",
    "ember-source": "~3.9.1",
    "ember-tabbable": "^0.1.3",
    "ember-truth-helpers": "^2.1.0",
    "ember-xregexp": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-ember": "^6.4.1",
    "eslint-plugin-mocha": "^5.3.0",
    "faker": "^4.1.0",
    "js-yaml": "^3.13.1",
    "loader.js": "^4.7.0",
