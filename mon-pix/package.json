{
  "name": "mon-pix",
  "version": "2.61.0",
  "private": false,
  "description": "Plateforme d'évaluation et de certification des compétences numériques à l'usage de tous les citoyens francophones.",
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "ci:test": "COVERAGE=true ember test --reporter dot",
    "clean": "rm -rf tmp dist node_modules",
    "coverage": "npm run coverage:check && npm run coverage:convert && npm run coverage:rename",
    "coverage:check": "COVERAGE=true ember test --reporter dot",
    "coverage:convert": "node ../scripts/lcov-path-converter $PWD",
    "coverage:rename": "mv coverage/lcov.info ../coverage/mon-pix_lcov.info",
    "dev": "ember server",
    "lint": "eslint app mirage tests",
    "lint:fix": "npm run lint -- --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "build": "ember build --environment $BUILD_ENVIRONMENT",
    "preinstall": "test \"$(npm --version)\" = 6.4.1",
    "scalingo-post-ra-creation": "echo 'nothing to do'",
    "start": "ember server --proxy",
    "test": "ember test --reporter dot",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "devDependencies": {
    "@ember/jquery": "^0.6.0",
    "@ember/optional-features": "^0.7.0",
    "@fortawesome/ember-fontawesome": "^0.1.13",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-regular-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "bootstrap": "^3.4.1",
    "broccoli-asset-rev": "^3.0.0",
    "chai-dom": "^1.8.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.4.0",
    "ember-burger-menu": "^3.3.3",
    "ember-cli": "~3.9.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-circleci": "^0.1.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^2.0.5",
    "ember-cli-htmlbars-inline-precompile": "^1.0.5",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-matomo-tag-manager": "^1.1.2",
    "ember-cli-mirage": "^1.0.0",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sentry": "^4.1.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-showdown": "^4.4.4",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-click-outside": "^1.0.4",
    "ember-collapsible-panel": "^3.2.1",
    "ember-component-focus": "^0.2.3",
    "ember-data": "~3.9.3",
    "ember-exam": "^2.1.5",
    "ember-export-application-global": "^2.0.0",
    "ember-group-by": "^0.0.6",
    "ember-keyboard": "^4.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-lodash": "^4.19.4",
    "ember-math-helpers": "^2.11.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-mocha": "^0.16.0",
    "ember-modal-dialog": "^2.4.4",
    "ember-moment": "^7.8.1",
    "ember-resolver": "^5.1.3",
