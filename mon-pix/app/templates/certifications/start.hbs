{{title pageTitle}}
<BurgerMenu @animation="push" @translucentOverlay={{true}} as |burger|>
  <burger.outlet>

    <NavbarHeader @burger={{burger}} />
    <PixBackgroundHeader>

      {{#if model.isCertifiable}}
        <PixBlock @shadow="heavy">
          <Stepper @steps={{array "certification-stepper-join" "certification-stepper-start"}}/>
        </PixBlock>
      {{else}}
        <CertificationNotCertifiable />
      {{/if}}

    </PixBackgroundHeader>

  </burger.outlet>
</BurgerMenu>
