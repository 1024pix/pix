{{page-title (t "pages.sitemap.title")}}
<BurgerMenu @animation="push" @translucentOverlay={{true}} as |burger|>
  <burger.outlet>

    <header role="banner">
      <NavbarHeader @burger={{burger}} />
    </header>

    <Sitemap::Content @model={{@model}} />

    <Footer />
  </burger.outlet>
</BurgerMenu>