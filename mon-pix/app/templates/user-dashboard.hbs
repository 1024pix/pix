{{page-title (t "pages.dashboard.title")}}
<BurgerMenu @animation="push" @translucentOverlay={{true}} as |burger|>
  <burger.outlet>

    <header role="banner">
      <NavbarHeader @burger={{burger}} />
    </header>

    <Dashboard::Content @model={{this.model}} />

    <Footer />
  </burger.outlet>
</BurgerMenu>