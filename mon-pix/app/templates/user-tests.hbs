{{page-title (t 'pages.user-tests.title')}}
<BurgerMenu @animation="push" @translucentOverlay={{true}} as |burger|>
  <burger.outlet>
    <NavbarHeader @burger={{burger}} />
    <PixBackgroundHeader>
      <div class="user-tests-banner">
        <img src="{{this.rootURL}}/images/user-tests/banner-icon.svg" class="user-tests-banner__icon" role="none" aria-hidden="true"/>
        <h3 class="user-tests-banner__title">{{t 'pages.user-tests.title'}}</h3>
      </div>
    </PixBackgroundHeader>
    <Footer />
  </burger.outlet>
</BurgerMenu>
