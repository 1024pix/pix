{{page-title (t 'pages.profile.title')}}
<BurgerMenu @animation="push" @translucentOverlay={{true}} as |burger|>
  <burger.outlet>

    <NavbarHeader @burger={{burger}} @campaignParticipations={{@model.campaignParticipations}}/>

    <ProfileContent @model={{@model}} />

    <Footer />
  </burger.outlet>
</BurgerMenu>
