  "scripts": {
    "clean": "rm -rf node_modules",
    "cache:refresh": "ts-node scripts/refresh-cache",
    "db:new-migration": "npx knex --knexfile db/knexfile.ts migrate:make $migrationname",
    "db:create": "ts-node scripts/database/create-database",
    "db:delete": "ts-node scripts/database/drop-database",
    "db:empty": "ts-node scripts/database/empty-database",
    "db:migrate": "TS_NODE_TRANSPILE_ONLY=true knex --knexfile db/knexfile.ts migrate:latest && node scripts/database/pg-boss-migration.js",
    "db:rollback:latest": "TS_NODE_TRANSPILE_ONLY=true knex --knexfile db/knexfile.ts migrate:down",
    "db:prepare": "npm run db:delete && npm run db:create && npm run db:migrate",
    "db:seed": "TS_NODE_TRANSPILE_ONLY=true knex --knexfile db/knexfile.ts seed:run",
    "db:reset": "npm run db:prepare && npm run db:seed",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:dependencies": "depcheck",
    "lint:database": "NODE_ENV=test npm run db:prepare && schemalint --config tests/acceptance/database/configuration.js",
    "lint:file-and-directory-names": "ls-lint",
    "lint:code": "eslint . --ext js --cache --cache-strategy content",
    "lint:code:uncached": "eslint . --ext js",
    "manual-lint:json": "eslint --cache --cache-strategy content --fix --ext .json translations/",
    "postdeploy": "npm run db:migrate",
    "preinstall": "npx check-engine",
    "scalingo-postbuild": "node scripts/generate-cron > cron.json",
    "start": "ts-node index.js",
    "start:watch": "ts-node-dev index.js",
    "test": "NODE_ENV=test npm run db:prepare && npm run test:api",
    "test:api": "status=0; npm run test:api:unit || status=1 ; for dir in $(find tests/* -maxdepth 0 -type d -not -path tests/unit) ; do npm run test:api:path -- $dir'/**/*.{ts,js}' || status=1 ; done ; exit $status",
    "test:api:path": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --exit --recursive --reporter=${MOCHA_REPORTER:-dot} -r ts-node/register",
    "test:api:unit": "TEST_DATABASE_URL=postgres://SHOULD_NOT_REACH_DB_IN_UNIT_TESTS REDIS_URL= npm run test:api:path -- 'tests/unit/**/*.{ts,js}'",
    "test:api:integration": "npm run test:api:path -- 'tests/integration/**/*.{ts,js}'",
    "test:api:acceptance": "npm run test:api:path -- 'tests/acceptance/**/*.{ts,js}'",
    "test:api:debug": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --inspect-brk=9229 --recursive --exit --reporter dot -r ts-node/register tests/**/*.{ts,js} ",
    "test:api:watch": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --recursive tests --watch --reporter dot -r ts-node/register tests/**/*.{ts,js}",
    "test:api:bail": "npm run test:api:unit -- --bail && npm run test:api:integration -- --bail && npm run test:api:acceptance -- --bail",
    "test:ci": "npm run test",
    "test:lint": "npm test && npm run lint",
    "ts-migrate": "ts-migrate",
    "migrate:typescript": "cjs-to-es6 lib/domain/models && npm run ts-migrate -- rename ./ -s lib/domain/models && npm run ts-migrate -- migrate ./ -s lib/domain/models && ts-node migrate.ts && eslint --fix ."
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "watch": [
      "lib",
      "index.js",
      "server.js",
      "package.json",
      "db",
      "translations",
      ".env"
    ],
    "ignore": [
      "db/seeds",
      "db/migrations",
      "db/database-builder"
    ],
    "ext": "js"
  }
}
