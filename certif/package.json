    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.8.0",
    "ember-cli": "^3.23.0",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-clipboard": "^0.15.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-matomo-tag-manager": "^1.2.4",
    "ember-cli-mirage": "^1.1.8",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-notifications": "^6.3.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^2.0.2",
    "ember-cli-terser": "^4.0.1",
    "ember-composable-helpers": "^4.3.0",
    "ember-data": "~3.23.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-file-upload": "^2.7.1",
    "ember-flatpickr": "^2.15.4",
    "ember-inputmask": "^0.11.0",
    "ember-keyboard": "^6.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-modal-dialog": "^3.0.0-beta.4",
    "ember-moment": "^8.0.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-simple-auth": "^3.0.1",
    "ember-sinon": "^5.0.0",
    "ember-source": "^3.23.1",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^7.15.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-mocha": "^8.0.0",
    "faker": "^5.1.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.15",
    "p-queue": "^6.3.0",
    "pix-ui": "git://github.com/1024pix/pix-ui.git#v2.0.3",
    "qunit-dom": "^1.6.0",
    "sass": "^1.26.3",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  }
