{
  "name": "pix-certif",
  "version": "3.232.0",
  "private": false,
  "description": "Plateforme en ligne de gestion des sessions de certification",
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "16.14.0",
    "npm": ">=8.3.1 <=8.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "ember": {
    "edition": "octane"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment $BUILD_ENVIRONMENT && npm run build:uncached:assets",
    "build:uncached:assets": "cp dist/assets/pix-certif-*.css dist/assets/pix-certif.uncached.css && cp dist/assets/vendor-*.css dist/assets/vendor.uncached.css",
    "clean": "rm -rf tmp dist node_modules",
    "dev": "ember serve",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --cache --cache-strategy content",
    "lint:js:uncached": "eslint .",
    "lint:scss": "stylelint app/styles/**/*.scss",
    "preinstall": "npx check-engine",
    "start": "ember serve --proxy http://localhost:3000",
    "test": "ember test --reporter dot",
    "test:ci": "npm run test",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "devDependencies": {
    "@1024pix/ember-testing-library": "^0.5.0",
    "@1024pix/pix-ui": "^14.8.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@fortawesome/ember-fontawesome": "^0.2.3",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-api-actions": "^0.2.9",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "^3.28.5",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-clipboard": "^0.15.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-matomo-tag-manager": "^1.3.1",
    "ember-cli-mirage": "^2.4.0",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-notifications": "^8.0.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^4.3.0",
    "ember-data": "~3.28.6",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-file-upload": "^2.7.1",
    "ember-flatpickr": "^2.15.4",
    "ember-inputmask": "^0.11.0",
    "ember-intl": "^5.7.2",
    "ember-keyboard": "^6.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-moment": "^9.0.1",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-simple-auth": "^3.1.0",
    "ember-source": "^3.28.8",
    "ember-template-lint": "^4.0.0",
    "ember-template-lint-plugin-prettier": "^4.0.0",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "eslint-plugin-yml": "^0.11.0",
    "faker": "^5.5.2",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "p-queue": "^6.3.0",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "sass": "^1.32.8",
    "sinon": "^13.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0"
  }
}
