
{{challenge-instruction instruction=challenge._instructionAsObject}}

<div class="rounded-panel challenge-response">
  <div class="challenge-proposals">
    {{qroc-proposal 
        blocks=challenge._proposalsAsBlocks 
        answerValue=answers.value
        onInputChanged='inputChanged'}}
  </div>
</div>

{{#if errorMessage}}
  <div class="alert alert-danger" role="alert">
    {{errorMessage}}
  </div>
{{/if}}

{{#if assessment}}
  {{challenge-actionbar
      skip="skip"
      validate="validate"}}
{{/if}}
