<div class="comparison-window">
  <div class="routable-modal--dialog comparison-window--dialog">

    <div class="routable-modal--content comparison-window--content">

      <div class="routable-modal--header comparison-window__header">
        {{#routable-modal-close-button class="routable-modal--close-button"}}

          <div class="close-button-container">
            <div>fermer</div>
            <div>
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 0 24 27"><path d="M12,20C7.59,20 4,16.41 4,12C4,7.59 7.59,4 12,4C16.41,4 20,7.59 20,12C20,16.41 16.41,20 12,20M12,2C6.47,2 2,6.47 2,12C2,17.53 6.47,22 12,22C17.53,22 22,17.53 22,12C22,6.47 17.53,2 12,2M14.59,8L12,10.59L9.41,8L8,9.41L10.59,12L8,14.59L9.41,16L12,13.41L14.59,16L16,14.59L13.41,12L16,9.41L14.59,8Z" fill="white"/>
              </svg>
            </div>
          </div>
        {{/routable-modal-close-button}}
        <div class="comparison-window__result-item-index">
          {{index}}
        </div>
        <div class="comparison-window__result-item-line">
        </div>

        <div class="comparison-window__title">
          <div data-toggle="tooltip" data-placement="top" title="{{resultItemContent.titleTooltip}}">
            {{#if resultItemContent.custom}}
              <svg width="22" height="22" viewBox="0 0 24 25" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><defs><path d="{{resultItemContent.path}}" id="path-1"></path><mask id="mask-2" maskContentUnits="userSpaceOnUse" maskUnits="objectBoundingBox" x="0" y="0" width="24" height="24" fill="white"><use xlink:href="#path-1"></use></mask></defs><g id="Résultats" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g id="test-fin-1" transform="translate(-1155.000000, -532.000000)" stroke="#FFBE00" stroke-width="14"><g id="Group3" transform="translate(226.000000, 504.000000)"><use id="Shape-Copy-3" mask="url(#mask-2)" xlink:href="#path-1"></use></g></g></g></svg>
            {{else}}
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 0 24 24"><path d="{{resultItemContent.path}}" fill="{{resultItemContent.color}}"></path></svg>
            {{/if}}
          </div>
        </div>
        <div class="comparison-window__title-text">{{resultItemContent.title}}</div>
      </div>

      <div class="routable-modal--body comparison-window--body">

        <div class="rounded-panel comparison-window__instruction">
          <div class="rounded-panel__row ">
            {{markdown-to-html
              class="challenge-statement__instruction"
              extensions='targetBlank'
              markdown=challenge.instruction}}
          </div>

          {{#if challenge.illustrationUrl}}
            <div class="rounded-panel__row challenge-statement__illustration-section">
              <img class="challenge-statement__illustration" src="{{ challenge.illustrationUrl }}" alt="Illustration de l'épreuve">
            </div>
          {{/if}}
        </div>

        {{#if isAssessmentChallengeTypeQcm}}
          <div class="rounded-panel comparison-window__proposals comparison-window__corrected-answers comparison-window__corrected-answers--qcm">
            <div class="rounded-panel__row">
              {{#each labeledCheckboxes as |labeledCheckbox index|}}
                <p class="challenge-response__proposal proposal-comparison-window proposal-paragraph">
                  {{!-- checkbox --}}
                  <input class="comparison-window-boolean checkbox-comparison-window" type="checkbox" disabled="disabled" name="{{inc index}}" checked={{labeledCheckbox.[1]}}/>
                  <label class="challenge-response__proposal-label label-checkbox-proposal--comparison">
                    {{!-- text of proposal  --}}
                    <span class="comparison-window-oracle" data-goodness="{{if (get solutionArray (concat index)) 'good' 'bad'}}" data-checked={{if labeledCheckbox.[1] 'yes' 'no'}}>
                      {{labeledCheckbox.[0]}}
                    </span>
                    {{!-- SVG to represent on / off state --}}
                    <svg class="checkbox-disabled-on picture-checkbox-proposal--comparison" width="18px" height="18px" viewBox="0 0 18 18" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g id="Styles" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g id="Icons" transform="translate(-293.000000, -89.000000)" fill="#7D808B"><g id="icontest--checkbox-disabled" transform="translate(292.000000, 88.000000)"><path d="M8,15 L3,10 L4.41,8.58 L8,12.17 L15.59,4.58 L17,6 L8,15 Z M17,1 L3,1 C1.89,1 1,1.89 1,3 L1,17 C1,18.1045695 1.8954305,19 3,19 L17,19 C18.1045695,19 19,18.1045695 19,17 L19,3 C19,1.89 18.1,1 17,1 Z" id="Shape"></path></g></g></g></svg>
                    <svg class="checkbox-disabled-off picture-checkbox-proposal--comparison" width="18px" height="18px" viewBox="0 0 18 18" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g id="Styles" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g id="Icons" transform="translate(-213.000000, -89.000000)" fill="#7D808B"><g id="icontest--checkbox-off" transform="translate(212.000000, 88.000000)"><path d="M17,1 L3,1 C1.89,1 1,1.89 1,3 L1,17 C1,18.1045695 1.8954305,19 3,19 L17,19 C18.1045695,19 19,18.1045695 19,17 L19,3 C19,1.89 18.1,1 17,1 L17,1 Z M17,3 L17,17 L3,17 L3,3 L17,3 Z" id="Shape"></path></g></g></g></svg>
                  </label>
                </p>
              {{/each}}
            </div>
          </div>
        {{/if}}

        {{#if isAssessmentChallengeTypeQroc}}
          <div class="comparison-window__corrected-answers comparison-window__corrected-answers--qroc">
            {{qroc-solution-panel answer=answer solution=solution }}
          </div>
        {{/if}}

      </div>

      <div class="routable-modal--footer">
        <div class="comparison-window__feedback-panel">
          {{feedback-panel answer}}
        </div>
      </div>
    </div>
  </div>
</div>
