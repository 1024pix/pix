<div class="comparison-window">
  <div class="routable-modal--dialog comparison-window--dialog">

    <div class="routable-modal--content comparison-window--content">

      <div class="routable-modal--header comparison-window--header">
        {{#routable-modal-close-button class="routable-modal--close-button"}}

        <div class="close-button-container">
          <div>fermer </div>
          <div><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 0 24 27"><path d="M12,20C7.59,20 4,16.41 4,12C4,7.59 7.59,4 12,4C16.41,4 20,7.59 20,12C20,16.41 16.41,20 12,20M12,2C6.47,2 2,6.47 2,12C2,17.53 6.47,22 12,22C17.53,22 22,17.53 22,12C22,6.47 17.53,2 12,2M14.59,8L12,10.59L9.41,8L8,9.41L10.59,12L8,14.59L9.41,16L12,13.41L14.59,16L16,14.59L13.41,12L16,9.41L14.59,8Z" fill="white" /></svg></div>
        </div>
        {{/routable-modal-close-button}}
        <div class="assessment-results-result-index">
          {{index}}
        </div>
        <div class="assessment-results-result-light-line">
        </div>

        <div class="assessment-results-result-titre">
          {{#if answer.isResultOk}}
            <div data-toggle="tooltip" data-placement="top" title="Réponse correcte">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 -2 24 24"><path d="M12,2A10,10 0 0,1 22,12A10,10 0 0,1 12,22A10,10 0 0,1 2,12A10,10 0 0,1 12,2M11,16.5L18,9.5L16.59,8.09L11,13.67L7.91,10.59L6.5,12L11,16.5Z" fill="#30d5b0"/></svg>
            </div>
            <div class="assessment-results-result-text">Vous avez la bonne réponse !</div>

          {{else if answer.isResultNotOk}}
            <div data-toggle="tooltip" data-placement="top" title="Réponse incorrecte">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24px" height="24px" viewBox="0 -2 24 24" ><path d="M12,2C17.53,2 22,6.47 22,12C22,17.53 17.53,22 12,22C6.47,22 2,17.53 2,12C2,6.47 6.47,2 12,2M15.59,7L12,10.59L8.41,7L7,8.41L10.59,12L7,15.59L8.41,17L12,13.41L15.59,17L17,15.59L13.41,12L17,8.41L15.59,7Z" fill="#ff4600" /></svg>
            </div>
            <div class="assessment-results-result-text">Vous n'avez pas la bonne réponse</div>

          {{else if answer.isResultWithoutAnswer}}
            <div data-toggle="tooltip" data-placement="top" title="Sans réponse">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 -2 24 24"><path d="M12,2A10,10 0 0,0 2,12A10,10 0 0,0 12,22A10,10 0 0,0 22,12A10,10 0 0,0 12,2M8,8L13,12L8,16M14,8H16V16H14" fill="#3e4149"/></svg>
            </div>
            <div class="assessment-results-result-text">Vous n'avez pas donné de réponse</div>

          {{else if answer.isResultTimedOut}}
            <div data-toggle="tooltip" data-placement="top" title="Temps dépassé">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 -2 24 24"><path d="M11,17A1,1 0 0,0 12,18A1,1 0 0,0 13,17A1,1 0 0,0 12,16A1,1 0 0,0 11,17M11,3V7H13V5.08C16.39,5.57 19,8.47 19,12A7,7 0 0,1 12,19A7,7 0 0,1 5,12C5,10.32 5.59,8.78 6.58,7.58L12,13L13.41,11.59L6.61,4.79V4.81C4.42,6.45 3,9.05 3,12A9,9 0 0,0 12,21A9,9 0 0,0 21,12A9,9 0 0,0 12,3M18,12A1,1 0 0,0 17,11A1,1 0 0,0 16,12A1,1 0 0,0 17,13A1,1 0 0,0 18,12M6,12A1,1 0 0,0 7,13A1,1 0 0,0 8,12A1,1 0 0,0 7,11A1,1 0 0,0 6,12Z" fill="red"/></svg>
            </div>
            <div class="assessment-results-result-text">Vous avez dépassé le temps imparti</div>

          {{else}}
            <div data-toggle="tooltip" data-placement="top" data-lines="2" title="Correction automatique en cours de développement ;)">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="24" height="24" viewBox="0 -2 24 24"><path d="M13,13H11V7H13M13,17H11V15H13M12,2A10,10 0 0,0 2,12A10,10 0 0,0 12,22A10,10 0 0,0 22,12A10,10 0 0,0 12,2Z" fill="#446eff"/></svg>
            </div>
          {{/if}}
        </div>
      </div>

      <div class="routable-modal--body comparison-window--body">

        <div class="rounded-panel comparison-window__instruction">
          <div class="rounded-panel__row ">
            {{markdown-to-html
                class="challenge-statement__instruction"
                extensions='targetBlank'
                markdown=challenge.instruction}}
          </div>

         {{#if challenge.illustrationUrl}}
         <div class="rounded-panel__row challenge-statement__illustration-section">
           <img class="challenge-statement__illustration"
                src="{{ challenge.illustrationUrl }}"
                alt="Illustration de l'épreuve">
         </div>
         {{/if}}
        </div>

        <div class="rounded-panel comparison-window__proposals">
          <div class="rounded-panel__row ">
            {{#each labeledCheckboxes as |labeledCheckbox index|}}
            <p class="challenge-response__proposal">
              <label class="challenge-response__proposal-label">
                {{!-- checkbox --}}
                <input class="comparison-window-boolean"
                       type="checkbox"
                       disabled="disabled"
                       name="{{inc index}}"
                       checked={{labeledCheckbox.[1]}}
                />
                {{!-- text of proposal  --}}
                <span
                  class="comparison-window-oracle"
                  data-goodness="{{if (get solutionArray (concat index)) 'good' 'bad'}}"
                  data-checked={{if labeledCheckbox.[1] 'yes' 'no'}}
                >
                    {{labeledCheckbox.[0]}}
                </span>
              </label>
            </p>
            {{/each}}
          </div>
        </div>
      </div>

      <div class="routable-modal--footer comparison-window--footer">
        {{feedback-panel answer}}
      </div>
    </div>
  </div>
</div>
