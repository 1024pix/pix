<Challenge::ChallengeStatement @challenge={{@challenge}} />
<Challenge::ChallengeItemAutoReply
  @challenge={{@challenge}}
  @assessment={{@assessment}}
  @answerValidated={{this.answerValidated}}
/>
<AnswerFeedback
  @answer={{this.answer}}
  @answerHasBeenValidated={{this.answerHasBeenValidated}}
  @nextAction={{this.resume}}
/>