{
  "name": "pix-admin",
  "version": "1.2.1",
  "private": true,
  "description": "Interface d'administration pour les Pix Masters.",
  "license": "AGPL-3.0",
  "author": "Team Pix",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/1024pix/admin",
  "scripts": {
    "build": "npx ember build",
    "dev": "npx ember serve",
    "lint:js": "eslint ./*.js app config lib server tests",
    "test": "npx ember exam",
    "coverage": "COVERAGE=true npx ember test",
    "publish": "npm run publish:minor",
    "publish:patch": "npm version patch -m \"Release v%s\"",
    "publish:minor": "npm version minor -m \"Release v%s\"",
    "publish:major": "npm version major -m \"Release v%s\"",
    "preversion": "git checkout dev && git pull && git fetch -t && npm test",
    "postversion": "git push origin dev && git checkout master && git pull && git merge --no-ff --no-edit dev && git push origin master && git push scalingo master && git push --tags && git checkout dev",
    "scalingo-postbuild": "npx ember build --environment $BUILD_ENVIRONMENT",
    "start": "ember serve --proxy"
  },
  "devDependencies": {
    "bootstrap": "^4.1.1",
    "broccoli-asset-rev": "^2.7.0",
    "ember-ajax": "^3.1.0",
    "ember-auto-import": "^1.2.2",
    "ember-bootstrap": "^1.2.2",
    "ember-cli": "^3.1.4",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^6.14.1",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^2.1.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-notifications": "^4.3.3",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sass": "^7.2.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-composable-helpers": "^2.1.0",
    "ember-data": "~3.1.1",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-file-upload": "^2.5.3",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-metrics": "poteto/ember-metrics#master",
    "ember-modal-dialog": "^3.0.0-beta.0",
    "ember-models-table": "^2.6.1",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-power-select": "^2.0.3",
    "ember-resolver": "^4.5.6",
    "ember-simple-auth": "^1.6.0",
    "ember-source": "^3.2.0",
    "ember-truth-helpers": "^2.0.0",
    "emberx-select": "^3.1.1",
    "eslint-plugin-ember": "^5.2.0",
    "loader.js": "^4.7.0",
    "popper.js": "^1.14.3",
    "qunit-dom": "^0.6.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || 7.* || 8.* || 9.*"
  },
  "dependencies": {
    "file-saver": "^1.3.8",
    "json2csv": "^4.1.6",
    "papaparse": "^4.5.0"
  }
}
