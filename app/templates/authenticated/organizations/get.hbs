<header class="page-header">
  <div class="page-title">
    {{link-to 'Toutes les organisations' 'authenticated.organizations.list'}}
    <span style="color: #DDDDDD;">&nbsp;>&nbsp;</span>
    {{model.name}}
  </div>
  <div class="page-actions">
    <button class="btn btn-outline-default btn-thin hidden" {{action "toggleWipModal"}}>
      {{fa-icon "edit"}} Modifier
    </button>
    <button class="btn btn-outline-default btn-thin hidden" {{action "toggleWipModal"}}>
      {{fa-icon "trash"}} Supprimer
    </button>
  </div>
</header>

<main class="page-body" id="organizations-get-page">

  {{organization-information-section organization=model}}

  {{organization-members-section organization=model}}

</main>
