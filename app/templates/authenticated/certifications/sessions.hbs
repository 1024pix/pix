<header class="page-header">
  <h1 class="page-title">Sessions de certification</h1>
  <div class="page-actions">
    {{certification-select id=(readonly sessionId) select=(action "onLoadSession")}}
  </div>
</header>
{{#if sessionId}}
  {{#bs-nav type="tabs" as |nav|}}
    {{#nav.item}}{{#nav.link-to 'authenticated.certifications.sessions.info.index' sessionId}}Informations{{/nav.link-to}}{{/nav.item}}
    {{#nav.item}}{{#nav.link-to 'authenticated.certifications.sessions.info.list' sessionId}}Liste{{/nav.link-to}}{{/nav.item}}
  {{/bs-nav}}
{{/if}}
<section class="page-section  {{if sessionId 'with-navs'}}">
  {{#if loading}}
    <div class="single-loading">
      <i class='fa fa-spinner fa-spin fa-lg'></i>
      Chargement...
    </div>
  {{else}}
    {{outlet}}
  {{/if}}
</section>
