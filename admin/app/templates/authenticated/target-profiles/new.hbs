{{page-title "Nouveau profil cible"}}
<header class="page-header">
  <div class="page-title">
    <h1>Nouveau profil cible</h1>
  </div>
</header>

<main class="page-body">
  <section class="page-section">
    <TargetProfiles::CreateTargetProfileForm
      @targetProfile={{this.model.targetProfile}}
      @frameworks={{this.model.frameworks}}
      @onSubmit={{this.createTargetProfile}}
      @onCancel={{this.goBackToTargetProfileList}}
    />
  </section>
</main>