{{page-title "Nouveau profil cible"}}
<header class="page-header">
  <div class="page-title">
    Nouveau profil cible
  </div>
</header>

<main class="page-body">
  <section class="page-section">
    <TargetProfiles::CreateTargetProfileForm
      @targetProfile={{this.model.targetProfile}}
      @frameworks={{this.model.frameworks}}
      @selectedTubeIds={{this.selectedTubeIds}}
      @areas={{this.areas}}
      @tubeLevels={{this.tubeLevels}}
      @refreshAreas={{this.refreshAreas}}
      @isDisabled={{this.isSaving}}
      @onSubmit={{this.createTargetProfile}}
      @onCancel={{this.goBackToTargetProfileList}}
    />
  </section>
</main>