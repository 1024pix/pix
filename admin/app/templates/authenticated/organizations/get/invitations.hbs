{{page-title "Orga " @model.organization.id " | Invitations"}}
{{#if this.accessControl.hasAccessToOrganizationActionsScope}}
  <Organizations::InvitationsAction
    @userEmailToInvite={{this.userEmailToInvite}}
    @onChangeUserEmailToInvite={{this.onChangeUserEmailToInvite}}
    @userEmailToInviteError={{this.userEmailToInviteError}}
    @createOrganizationInvitation={{this.createOrganizationInvitation}}
  />
{{/if}}
<Organizations::Invitations @invitations={{@model.organizationInvitations}} />