<td>
  <Input
    {{on "input" (fn @updateSelectedTubes @tube.id)}}
    data-thematic={{@thematic.id}}
    data-competenceId={{@competence.id}}
    data-tube={{@tube.id}}
    id="tube-{{@tube.id}}"
    @type="checkbox"
  />
  <label for="tube-{{@tube.id}}">
    {{@tube.practicalTitle}}
    :
    {{@tube.practicalDescription}}
  </label>
</td>
<td class="table__column--center">
  <PixSelect
    id="select-level-tube-{{@tube.id}}"
    @options={{this.levelOptions}}
    @onChange={{this.setLevelTube}}
    @selectedOption={{8}}
    disabled={{this.isNotSelected}}
  />
  <label for="select-level-tube-{{@tube.id}}" class="sr-only">SÃ©lection du niveau du sujet</label>
</td>
<td class="table__column--center">
  <div class="icon-container" aria-label="{{if @tube.mobile "compatible mobile" "incompatible mobile"}}">
    <FaIcon @icon="mobile-alt" class="fa-2x {{if @tube.mobile "is-responsive"}}" />
    {{#unless @tube.mobile}}
      <FaIcon @icon="slash" class="fa-2x not-responsive" />
    {{/unless}}
  </div>
  <div class="icon-container" aria-label="{{if @tube.tablet "compatible tablette" "incompatible tablette"}}">
    <FaIcon @icon="tablet-alt" class="fa-2x {{if @tube.tablet "is-responsive"}}" />
    {{#unless @tube.tablet}}
      <FaIcon @icon="slash" class="fa-2x not-responsive" />
    {{/unless}}
  </div>
</td>