{{#if @campaign.hasStages}}
  <Campaign::Charts::ParticipantsByStage
    @campaignId={{@campaign.id}}
    @onSelectStage={{@onSelectStage}}
    class="assessment-results__charts hide-on-mobile"
  />
{{else}}
  <Campaign::Charts::ParticipantsByMasteryPercentage
    @campaignId={{@campaign.id}}
    class="assessment-results__charts hide-on-mobile"
  />
{{/if}}