<Ui::IndicatorCard
  @title={{t "cards.participants-average-stages.title"}}
  @icon="crown"
  @color="blue"
  @info={{t "cards.participants-average-stages.information"}}
  @isLoading={{@isLoading}}
  ...attributes
>
  <PixStars
    @count={{this.starsAcquired}}
    @total={{this.starsTotal}}
    @alt={{this.altMessage}}
    @color="blue"
    class="stage-average-stars"
  />
</Ui::IndicatorCard>