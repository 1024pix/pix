{{page-title (t "pages.sup-organization-participants-import.title")}}

<PixReturnTo @route="authenticated.sup-organization-participants.list">
  {{t "common.actions.back"}}
</PixReturnTo>

{{#if this.currentUser.isSUPManagingStudents}}
  <SupOrganizationParticipant::Import
    @onImportStudents={{this.importStudents}}
    @onReplaceStudents={{this.replaceStudents}}
    @isLoading={{this.isLoading}}
  />
{{else if this.currentUser.isSCOManagingStudents}}
  <ScoOrganizationParticipant::Import @isLoading={{this.isLoading}} />
{{/if}}